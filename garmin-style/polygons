landuse=meadow & meadow=perpetual    { name '${name} (secena)'      | 'Secena louka' }      [0x10f01 level 4]
landuse=meadow & meadow=transitional { name '${name} (zarustajici)' | 'Zarustajici louka' } [0x10f01 level 4]
landuse=meadow [0x10f01 level 4]


